includes:
	- vendor/cakedc/cakephp-phpstan/extension.neon
parameters:
	level: 6
	treatPhpDocTypesAsCertain: false
	bootstrapFiles:
		- config/bootstrap.php
	paths:
		- config
		- src
		- templates
		- tests
	ignoreErrors:
		- identifier: missingType.iterableValue
		- identifier: missingType.generics
